@prefix : <http://example.com/BallpointPenOntology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://example.com/BallpointPenOntology#> .

<http://example.com/BallpointPenOntology> rdf:type owl:Ontology ;
                                           owl:versionIRI <http://example.com/BallpointPenOntology/1.0.0> ;
                                           rdfs:comment "A knowledge model for describing several types of ballpoint pens based on their components, characteristics and make." .

#################################################################
#    Object Properties
#################################################################

###  http://example.com/BallpointPenOntology/hasCharacteristic
<http://example.com/BallpointPenOntology/hasCharacteristic> rdf:type owl:ObjectProperty ;
                                                            rdfs:subPropertyOf <http://example.com/BallpointPenOntology/hasFeature> ;
                                                            owl:inverseOf <http://example.com/BallpointPenOntology/isCharacteristicOf> ;
                                                            rdfs:domain <http://example.com/BallpointPenOntology/Pen> ;
                                                            rdfs:range <http://example.com/BallpointPenOntology/PenCharacteristic> .


###  http://example.com/BallpointPenOntology/hasComponent
<http://example.com/BallpointPenOntology/hasComponent> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf <http://example.com/BallpointPenOntology/hasFeature> ;
                                                       owl:inverseOf <http://example.com/BallpointPenOntology/isComponentOf> ;
                                                       rdfs:domain <http://example.com/BallpointPenOntology/Pen> ;
                                                       rdfs:range <http://example.com/BallpointPenOntology/PenComponent> .


###  http://example.com/BallpointPenOntology/hasFeature
<http://example.com/BallpointPenOntology/hasFeature> rdf:type owl:ObjectProperty ;
                                                     owl:inverseOf <http://example.com/BallpointPenOntology/isFeatureOf> ;
                                                     rdf:type owl:TransitiveProperty .


###  http://example.com/BallpointPenOntology/hasMake
<http://example.com/BallpointPenOntology/hasMake> rdf:type owl:ObjectProperty ,
                                                           owl:FunctionalProperty .


###  http://example.com/BallpointPenOntology/isCharacteristicOf
<http://example.com/BallpointPenOntology/isCharacteristicOf> rdf:type owl:ObjectProperty ;
                                                             rdfs:subPropertyOf <http://example.com/BallpointPenOntology/isFeatureOf> .


###  http://example.com/BallpointPenOntology/isComponentOf
<http://example.com/BallpointPenOntology/isComponentOf> rdf:type owl:ObjectProperty ;
                                                        rdfs:subPropertyOf <http://example.com/BallpointPenOntology/isFeatureOf> .


###  http://example.com/BallpointPenOntology/isFeatureOf
<http://example.com/BallpointPenOntology/isFeatureOf> rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://example.com/BallpointPenOntology/hasWidestDiameterInMillimetres
<http://example.com/BallpointPenOntology/hasWidestDiameterInMillimetres> rdf:type owl:DatatypeProperty ,
                                                                                  owl:FunctionalProperty ;
                                                                         rdfs:range xsd:float .


#################################################################
#    Classes
#################################################################

###  http://example.com/BallpointPenOntology/BallPoint
<http://example.com/BallpointPenOntology/BallPoint> rdf:type owl:Class ;
                                                    rdfs:subClassOf <http://example.com/BallpointPenOntology/PenComponent> .


###  http://example.com/BallpointPenOntology/BallpointPen
<http://example.com/BallpointPenOntology/BallpointPen> rdf:type owl:Class ;
                                                       rdfs:subClassOf <http://example.com/BallpointPenOntology/Pen> ,
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty <http://example.com/BallpointPenOntology/hasComponent> ;
                                                                         owl:someValuesFrom <http://example.com/BallpointPenOntology/BallPoint>
                                                                       ] ,
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty <http://example.com/BallpointPenOntology/hasComponent> ;
                                                                         owl:someValuesFrom <http://example.com/BallpointPenOntology/InkReservoir>
                                                                       ] ,
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty <http://example.com/BallpointPenOntology/hasComponent> ;
                                                                         owl:someValuesFrom <http://example.com/BallpointPenOntology/Socket>
                                                                       ] ;
                                                       rdfs:comment "A ballpoint pen, also known as 'biro' and 'ball pen' is a pen that dispenses ink over a metal ball at its point, i.e. over a 'ball point' (Wikipedia)." .


###  http://example.com/BallpointPenOntology/BicCristal
