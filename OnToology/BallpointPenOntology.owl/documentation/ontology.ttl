                                                     rdfs:subClassOf <http://example.com/BallpointPenOntology/BallpointPen> ,
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty <http://example.com/BallpointPenOntology/hasComponent> ;
                                                                       owl:allValuesFrom [ rdf:type owl:Class ;
                                                                                           owl:unionOf ( <http://example.com/BallpointPenOntology/BallPoint>
                                                                                                         <http://example.com/BallpointPenOntology/BlindCap>
                                                                                                         <http://example.com/BallpointPenOntology/Cap>
                                                                                                         <http://example.com/BallpointPenOntology/InkReservoir>
                                                                                                         <http://example.com/BallpointPenOntology/SinglePenBody>
                                                                                                         <http://example.com/BallpointPenOntology/Socket>
                                                                                                       )
                                                                                         ]
                                                                     ] ,
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty <http://example.com/BallpointPenOntology/hasCharacteristic> ;
                                                                       owl:hasValue <http://example.com/BallpointPenOntology/Disposable>
                                                                     ] ,
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty <http://example.com/BallpointPenOntology/hasMake> ;
                                                                       owl:hasValue <http://example.com/BallpointPenOntology/Bic>
                                                                     ] .


